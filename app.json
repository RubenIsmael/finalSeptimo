{
  "expo": {
    "name": "Cementerio San Agustín",
    "slug": "cementerio-san-agustin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cementerio-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#4682B4"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#4682B4",
          "imageWidth": 200,
          "resizeMode": "contain",
          "placement": "center"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicación accede a tus fotos para subir comprobantes de pago.",
          "cameraPermission": "La aplicación accede a tu cámara para tomar fotos de comprobantes de pago."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}